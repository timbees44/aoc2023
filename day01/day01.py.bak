words = {"one": 1, "two": 2, "three": 3, "four": 4, "five": 5, "six": 6, "seven": 7, "eight": 8, "nine": 9}

with open("input.txt", "r") as f:
    total = 0
    sub = [] 
    # part 1
    for line in f:
        for i in line:
            if i.isnumeric():
                sub.append(i) 
        total += int(sub[0] + sub[-1])
        sub = [] 

